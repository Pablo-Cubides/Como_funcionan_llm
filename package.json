{
  "name": "exploramodelo-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=packages/app",
    "build": "npm run build --workspace=packages/app",
    "start": "npm run start --workspace=packages/app",
    "test": "npm run test --workspace=packages/app",
    "test:unit": "npm run test:unit --workspace=packages/app",
    "lint": "npm run lint --workspace=packages/app",
    "e2e": "npm run e2e --workspace=packages/app",
    "embed:build": "npm run build --workspace=packages/embed",
    "embed:dev": "npm run dev --workspace=packages/embed"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20"
  }
}
